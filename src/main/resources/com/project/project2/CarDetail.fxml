            <Image url="@Img/Car/KIA_Cerato_2021.jpg" />
         </image>
      </ImageView>
      <Label fx:id="label" layoutX="7.0" layoutY="113.0" prefHeight="34.0" prefWidth="202.0" text="License plate:" textFill="#da3737">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <JFXButton fx:id="addBtn" layoutX="92.0" layoutY="162.0" onAction="#addToContract" prefHeight="34.0" prefWidth="114.0" stylesheets="@Css/viewStyle.css" text="ADD" />
      <JFXButton fx:id="cancelBtn" layoutX="92.0" layoutY="162.0" onAction="#cancelAdd" prefHeight="34.0" prefWidth="114.0" style="-fx-background-color: #ff5656;" stylesheets="@Css/viewStyle.css" text="CANCLE" />
   </children>
