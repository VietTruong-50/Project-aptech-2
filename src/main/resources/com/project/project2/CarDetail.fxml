<AnchorPane prefHeight="207.0" prefWidth="157.0" style="-fx-background-radius: 20px;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.project2.controller.CarDetailController">
   <children>
      <ImageView fx:id="img" fitHeight="119.0" fitWidth="200.0" layoutX="6.0" layoutY="-6.0" pickOnBounds="true" preserveRatio="true">
